{
    "config-template.conf": {
        "output": "config.conf",
        "defaults": {
            "value1": "foo",
            "value2": "bar"
        },
        "profiles": {
            "p1": {
                "value1": "foo+bar"
            },
            "p2": {
                "expr": true
            },
            "p3": {
                "value2": "bar+foo",
                "expr": true
            }
        }
    }
}
